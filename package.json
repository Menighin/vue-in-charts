{
  "name": "vue-inline-chart",
  "version": "0.0.1",
  "description": "A Vue component to draw small charts inline and in tables",
  "main": "dist/main.bundle.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "lint": "eslint src/** test/*",
    "build-dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prd.js",
    "build-profile": "webpack --config webpack.prd.js --profile --json > stats.json",
    "test": "jest"
  },
  "keywords": [
    "vue",
    "chart",
    "inline"
  ],
  "files": [
    "dist/"
  ],
  "author": "Joao Menighin",
  "repository": {
    "type": "git",
    "url": "git://github.com/Menighin/vue-inline-chart.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@vue/server-test-utils": "^1.0.0-beta.13",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.8.3",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "vue": "^2.5.16",
    "vue-jest": "^2.3.0",
    "vue-loader": "^14.2.1",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
